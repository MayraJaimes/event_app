<div class="homeContainer">
    <h1>Home Page:</h1>
    <div class="create-event">
        <form class="form-create-event" id="create-event-form" name="create-event" method="POST" action="/#">
            <h2 class="form-event-heading">Input Event Information</h2>
            <label for="type-Event">
                <h5>What is the name of the event you will be hosting?</h5>
            </label>
            <input type="text" class="form-control" name="event-type" placeholder="Name of Event" required="" autofocus="" />
    
            <label for="type-Event">
                <h5>What is the date of the event?</h5>
            </label>
            <input type="date" class="form-control" name="date" placeholder="Date" required="" autofocus="" />
    
            <label for="type-Event">
                <h5>What is the location of the event?</h5>
            </label>
            <input type="text" class="form-control" name="location" placeholder="Location" required="" autofocus="" />
            <label for="type-Event">
                <h5>What is the start time of the event?</h5>
            </label>
            <input type="time" class="form-control" name="time" placeholder="Start Time" required="" autofocus="" />
            <label for="type-Event">
                <h5>What is the end time of the event?</h5>
            </label>
            <input type="time" class="form-control" name="time" placeholder="End Time" required="" autofocus="" />
    
            <button class="btn btn-lg btn-primary btn-block" type="submitEvent">Submit Event</button>
        </form>
    </div>

    
</div>





            